#include <iostream>


int main(void) {

  double num;
  std::cout << "Give numby";
  std::cin >> num;
  double root = num/2;

  while (std::abs(root*root-num)>0.000000001){
    root = 1/2*(root+(num/root));
  }

  std::cout << "The square root of "+std::to_string(num)+" is "+std::to_string(root);

  return 0;
}
